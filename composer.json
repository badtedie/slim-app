{
    "name": "itseasy/slim-app",
    "description": "Slim Application",
    "type": "library",
    "keywords": [
        "slim"
    ],
    "require": {
        "php" : "^7.4",
        "psr/cache": "^1.0",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.2",
        "php-di/php-di": "^6.3",
        "slim/php-view": "^3.0",
        "laminas/laminas-config-aggregator": "^1.4",
        "symfony/deprecation-contracts" : "^2.5",
        "symfony/console" : "^5.2",
        "symfony/http-foundation" : "^5.1",
        "symfony/security-csrf" : "^5.2",
        "laminas/laminas-navigation": "^2.10",
        "laminas/laminas-cache": "^2.11",
        "laminas/laminas-i18n": "^2.11",
        "laminas/laminas-view": "^2.12",
        "laminas/laminas-serializer": "^2.10",
        "laminas/laminas-log": "^2.13",
        "scssphp/scssphp": "^1.6"
    },
    "autoload" : {
        "psr-4" : {
            "Itseasy\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Itseasy\\Test\\" : "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wiryonolau",
            "email": "wiryonolau@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}

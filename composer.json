{
    "name": "itseasy/slim-app",
    "description": "Slim Application",
    "type": "library",
    "keywords": [
        "slim"
    ],
    "require": {
        "php" : "^7.2",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.2",
        "php-di/php-di": "^6.3",
        "slim/php-view": "^3.0",
        "symfony/http-foundation": "^5.1",
        "laminas/laminas-config-aggregator": "^1.4",
        "symfony/security-csrf": "^5.2",
        "symfony/console": "^5.2",
        "symfony/cache": "^5.2",
        "laminas/laminas-navigation": "^2.10"
    },
    "autoload" : {
        "psr-4" : {
            "Itseasy\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Itseasy\\Test\\" : "tests/"
        }
    },
